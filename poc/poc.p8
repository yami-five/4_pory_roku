    vm,vt=split(sub(vm,1,-2)),split(sub(vt,1,-2))
    draw_model(p,qt,vertices,vt,vm,faces,f,tc,uv,textures,true,32,false,1024)
end

function v3_len(vec)
    return sqrt(vec[1]*vec[1]+vec[2]*vec[2]+vec[3]*vec[3])        
end

function inf(t,x,y)
	x+=2*(cos(t))
	y+=2*cos(t)*sin(t)
    return x,y
end

